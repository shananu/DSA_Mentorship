import java.util.*;
public class A_1_5
{
    public static void main()
    {
       Scanner sc=new Scanner(System.in);
       System.out.println("Enter the number");
       int n= sc.nextInt();
       int d=1;
       while(n>9)
       {
           d++;
           n=n/10;
        }
       System.out.println("The number of digits "+d);
    }
}
