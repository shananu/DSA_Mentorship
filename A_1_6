import java.util.*;
public class A_1_6
{
    public static void main()
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number");
        int n= sc.nextInt();
        int d=0;
        //int c=0;
        //int k=0;
        //int b=n;
        while(n>0)
        {
         d=n%10;
         System.out.println(d);
         n=n/10;
        }
    }
}
